{
  "name": "node-vegeta",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "test": "$(npm bin)/nyc $(npm bin)/mocha --timeout 600000 test/**/*.test.ts",
    "publish": "npm run build && npm publish"
  },
  "dependencies": {
    "change-case": "^3.0.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "@types/power-assert": "^1.5.0",
    "espower-typescript": "^9.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "power-assert": "^1.6.1",
    "typescript": "^3.2.1"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "espower-typescript/guess"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
